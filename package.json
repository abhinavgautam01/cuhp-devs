{
  "name": "pending",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "generate-boilerplate": "pnpm --filter boilerplate-generator generate all",
    "problem-update:db": "pnpm --filter @repo/db update:problem",
    "problems-sync:db": "pnpm --filter @repo/db db:sync",
    "testcases-push:db": "pnpm --filter @repo/db push:testcases"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.9",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.30.1",
  "engines": {
    "node": ">=18"
  }
}
